{
  "name": "@ibis-app/api",
  "version": "0.1.2",
  "main": "dist/index.js",
  "bin": "start.js",
  "scripts": {
    "start": "node start.js dist",
    "test": "ava",
    "e2e": "ava --tap --serial e2e/index.test.js e2e/pkg.test.js",
    "build": "gulp build",
    "build:fast": "gulp buildFast",
    "pkg": "gulp package",
    "lint": "tslint --project . -t verbose",
    "watch": "gulp watch",
    "clean": "gulp clean"
  },
  "author": "Ben Spriggs",
  "license": "ISC",
  "dependencies": {
    "@ibis-app/lib": "^0.1.2",
    "axios": "^0.19.0",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "fuse.js": "^3.4.2",
    "nano": "^8.1.0",
    "node-html-parser": "^1.1.12",
    "write-file-atomic": "^3.0.0"
  },
  "pkg": {
    "scripts": [
      "out/*.js",
      "!out/*.test.js"
    ]
  },
  "paths": {
    "dist": "./dist",
    "tsconfig": "./tsconfig.json"
  },
  "ava": {
    "babel": false,
    "compileEnhancements": false,
    "tap": true,
    "files": [
      "dist/**/*.test.js"
    ],
    "timeout": "5s"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ibis-app/ibis.git"
  },
  "bugs": {
    "url": "https://github.com/ibis-app/ibis/issues"
  },
  "homepage": "https://github.com/ibis-app/ibis#readme",
  "description": "",
  "devDependencies": {
    "@types/nano": "^7.0.0"
  }
}
