{
  "name": "ibis-api",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "bin": "start.js",
  "scripts": {
    "start": "node start.js dist",
    "test": "ava",
    "e2e": "ava --tap --serial e2e/index.test.js e2e/pkg.test.js",
    "build": "gulp build",
    "build:fast": "gulp buildFast",
    "pkg": "gulp package",
    "lint": "tslint --project . -t verbose",
    "clean": "gulp clean"
  },
  "author": "Ben Spriggs",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.0",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "fuse.js": "^3.4.2",
    "ibis-lib": "file:../ibis-lib",
    "lowdb": "^1.0.0",
    "node-html-parser": "^1.1.12",
    "write-file-atomic": "^3.0.0"
  },
  "pkg": {
    "scripts": [
      "out/*.js",
      "!out/*.test.js"
    ]
  },
  "paths": {
    "dist": "./dist",
    "tsconfig": ["./tsconfig.json", "./tsconfig.js.json"]
  },
  "ava": {
    "babel": false,
    "compileEnhancements": false,
    "tap": true,
    "files": [
      "dist/**/*.test.js"
    ],
    "timeout": "5s"
  }
}
